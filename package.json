{
  "name": "@nethserver/vue-tailwind-lib",
  "private": false,
  "version": "0.0.27",
  "keywords": [
    "nethserver",
    "nethesis",
    "vuejs",
    "vue",
    "tailwind"
  ],
  "homepage": "https://github.com/NethServer/vue-tailwind-lib#readme",
  "bugs": "https://github.com/NethServer/dev/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/NethServer/vue-tailwind-lib.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-tailwind-lib.umd.js",
  "module": "./dist/vue-tailwind-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-tailwind-lib.es.js",
      "types": "./dist/main.d.ts",
      "require": "./dist/vue-tailwind-lib.umd.js"
    },
    "./dist/vue-tailwind-lib.css": {
      "import": "./dist/vue-tailwind-lib.css",
      "require": "./dist/vue-tailwind-lib.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "github:nethesis/Font-Awesome#solid",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@headlessui/vue": "^1.7.14",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "uid": "^2.0.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.15",
    "@storybook/addon-interactions": "^7.0.15",
    "@storybook/addon-links": "^7.0.15",
    "@storybook/blocks": "^7.0.15",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.15",
    "@storybook/vue3-vite": "^7.0.15",
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "path": "^0.12.7",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "storybook": "^7.0.15",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2"
  }
}
